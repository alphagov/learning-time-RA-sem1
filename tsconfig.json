{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": true,
    "target": "es2020",
    "resolveJsonModule": true,
    "outDir": "bin"
  }, 
  "include": ["src/**/*", "src/tests/**/*", "src/commands/.ts"]
}
