{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": true,
    "target": "es2020",
    "resolveJsonModule": true,
    "rootDir": "src", 
    "outDir": "dist"
  }, 
  "include": ["src/**/*", "src/tests/**/*"]
}
